&main_uart5 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&main_uart5_pins_default>;
};

&main_uart6 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&main_uart6_pins_default>;

	rs485-rts-delay = <1 1>;
	rs485-rts-active-high;
	rs485-rx-during-tx;
	rs485-rts-gpios = <&main_gpio0 9 GPIO_ACTIVE_HIGH>;
	rs485-term-gpios = <&main_gpio0 10 GPIO_ACTIVE_HIGH>;
	linux,rs485-enabled-at-boot-time;
};

